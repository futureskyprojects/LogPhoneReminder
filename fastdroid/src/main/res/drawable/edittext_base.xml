<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" android:enterFadeDuration="300" android:exitFadeDuration="300">
    <item android:state_focused="false">
        <shape>
            <solid android:color="#f3f6f9" />
            <stroke android:width="1dp" android:color="#f3f6f9" />
            <corners android:radius="10dp" />
            <padding android:bottom="@dimen/space_default" android:left="@dimen/space_default" android:right="@dimen/space_default" android:top="@dimen/space_default" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="#ebedf3" />
            <stroke android:width="1dp" android:color="#ebedf3" />
            <corners android:radius="10dp" />
            <padding android:bottom="@dimen/space_default" android:left="@dimen/space_default" android:right="@dimen/space_default" android:top="@dimen/space_default" />
        </shape>
    </item>
</selector>